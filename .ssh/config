Host gitlab.com
	HostName gitlab.com
	User git
	ForwardAgent yes
	IdentityFile ~/.ssh/gitlab_rsa
Host github.com
	HostName github.com
	User git
	ForwardAgent yes
	IdentityFile ~/.ssh/github_rsa
Host vs-ssh.visualstudio.com
  HostKeyAlgorithms=+ssh-rsa
	PubkeyAcceptedKeyTypes=ssh-rsa
	HostName vs-ssh.visualstudio.com
	User git
	IdentitiesOnly yes
	IdentityFile ~/.ssh/azure3_rsa
