{
  "id": "bks-er-diagram",
  "name": "ER Diagram",
  "author": {
    "name": "Beekeeper Studio",
    "url": "https://beekeeperstudio.io"
  },
  "description": "Interactive ER Diagram viewer for Beekeeper Studio to visualize and explore relational database schemas",
  "version": "1.0.3",
  "icon": "account_tree",
  "manifestVersion": 1,
  "minAppVersion": "5.5.0",
  "capabilities": {
    "views": [
      {
        "id": "main-view",
        "name": "ER Diagram",
        "type": "plain-tab",
        "entry": "dist/index.html"
      }
    ],
    "menu": [
      {
        "command": "showAllEntities",
        "name": "Open ER Diagram",
        "view": "main-view",
        "placement": "menubar.tools"
      },
      {
        "command": "showOneSchema",
        "name": "View ERD",
        "view": "main-view",
        "placement": "entity.schema.context"
      },
      {
        "command": "showOneTable",
        "name": "View ERD",
        "view": "main-view",
        "placement": "entity.table.context"
      },
      {
        "command": "showOneTable",
        "name": "View ERD",
        "view": "main-view",
        "placement": "structure.statusbar.menu"
      }
    ]
  }
}
